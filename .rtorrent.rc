port_range = 50000-51000
download_rate = 800
upload_rate = 70
max_open_files = 750
max_open_sockets = 250
# Watch for new torrents
schedule = watch_directory,5,5,load_start=/ed/bt/filelist/*.torrent
#schedule = tied_directory,5,5,start_tied=
# Stop all deleted torrents
schedule = untied_directory,5,5,stop_untied=
